@model IEnumerable<Employee.DAL.Entities.EmployeeN>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">@Html.DisplayNameFor(m => m.EmployeeID)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.FirstName)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.LastName)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.EmailAddress)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.Gender)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.Password)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.Salary)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.URL)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.isactive)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.isalive)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.HireDate)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.Country)</th>
            <th scope="col">@Html.DisplayNameFor(m => m.City)</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
        <tr>
            <th scope="col">@Html.DisplayFor(Model=>x.EmployeeID)</th>
            <th scope="col">@Html.DisplayFor(Model => x.FirstName)</th>
            <th scope="col">@Html.DisplayFor(Model => x.LastName)</th>
            <th scope="col">@Html.DisplayFor(Model => x.EmailAddress)</th>
            <th scope="col">@Html.DisplayFor(Model => x.Gender)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.Salary)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.URL)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.Password)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.isactive)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.isalive)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.HireDate)</th>
            <th scope="col">@Html.DisplayFor(Model=>x.Country)</th>
            <th scope="col">@Html.DisplayFor(Model => x.City)</th>

        </tr>
        }
     
    </tbody>
</table>

